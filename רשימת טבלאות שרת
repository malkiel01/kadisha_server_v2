   CREATE TABLE `mbeplusc_kadisha_v1`.`plots` (
     `id` INT(11) NOT NULL AUTO_INCREMENT,
     `plotNameHe` VARCHAR(255) NOT NULL,
     `plotNameEn` VARCHAR(255) NOT NULL,
     `plotLocation` VARCHAR(11) NOT NULL,
     `plotType` VARCHAR(11) NOT NULL,
     `nationalInsuranceCode` VARCHAR(11) NOT NULL,
     `plotCode` VARCHAR(11) NOT NULL,
     `blockId` INT(11) NOT NULL,
     `comments` VARCHAR(255) NOT NULL,
     `coordinates` VARCHAR(50) NOT NULL,
     `documentsList` TEXT NOT NULL,
     `isActive` BOOLEAN NOT NULL,
     `createdDate` VARCHAR(50) NOT NULL,
     `updateDate` VARCHAR(50) NOT NULL,
     `inactiveDate` VARCHAR(50) NOT NULL,
     PRIMARY KEY (`id`)
   ) ENGINE = InnoDB;

